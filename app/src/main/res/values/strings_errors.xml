    User does not have enough balance on account (prepaid).-->
    <string name="error_start_parking_not_enough_balance">Not enough amount on balance to start parking.</string>
    <!-- ANPR car not found. -->
    <string name="error_start_parking_no_anpr">ANPR car not found.</string>
    <!-- User does not have valid subscription. -->
    <string name="error_start_parking_no_subscription">Valid subscription is not found.</string>
